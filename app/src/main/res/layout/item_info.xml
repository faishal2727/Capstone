<?xml version="1.0" encoding="utf-8"?><androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="wrap_content"    android:layout_height="130dp"    android:layout_margin="4dp"    android:elevation="10dp"    app:cardElevation="10dp"    app:cardCornerRadius="10dp">    <androidx.constraintlayout.widget.ConstraintLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_margin="5dp">        <ImageView            android:id="@+id/ivEventList"            android:layout_width="100dp"            android:layout_height="70dp"            android:layout_marginTop="4dp"            android:src="@drawable/sample_event"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />        <TextView            android:id="@+id/tvNameEvent"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_margin="8dp"            android:fontFamily="@font/poppins"            android:maxLines="2"            android:text="Judul Event"            android:textColor="@color/black"            android:textSize="12sp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/ivEventList" />    </androidx.constraintlayout.widget.ConstraintLayout></androidx.cardview.widget.CardView>
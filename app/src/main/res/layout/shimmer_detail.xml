<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content">    <TextView        android:id="@+id/tvNameEventDetailS"        android:layout_width="150dp"        android:layout_height="wrap_content"        android:layout_marginStart="16dp"        android:layout_marginTop="16dp"        android:fontFamily="@font/poppins"        android:background="@color/gray_shimmer"        android:textColor="@color/black"        android:textSize="24sp"        android:textStyle="bold"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <TextView        android:id="@+id/dateEventS"        style="@style/AppTheme.TextMain"        android:layout_width="120dp"        android:layout_height="wrap_content"        android:layout_marginTop="8dp"        android:background="@color/gray_shimmer"        app:layout_constraintStart_toStartOf="@+id/tvNameEventDetailS"        app:layout_constraintTop_toBottomOf="@+id/tvNameEventDetailS" />    <TextView        android:id="@+id/tvDateEventS"        style="@style/AppTheme.TextMain"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:background="@color/gray_shimmer"        android:layout_marginStart="8dp"        android:layout_marginEnd="16dp"        app:layout_constraintBottom_toBottomOf="@+id/dateEventS"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toEndOf="@+id/dateEventS" />    <TextView        android:id="@+id/lokasiEventS"        style="@style/AppTheme.TextMain"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginStart="16dp"        android:background="@color/gray_shimmer"        android:layout_marginTop="4dp"        app:layout_constraintEnd_toEndOf="@+id/dateEventS"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/dateEventS" />    <TextView        android:id="@+id/tvDetailLocS"        style="@style/AppTheme.TextMain"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:background="@color/gray_shimmer"        android:layout_marginStart="8dp"        app:layout_constraintEnd_toEndOf="@+id/tvDateEventS"        app:layout_constraintStart_toEndOf="@+id/lokasiEventS"        app:layout_constraintTop_toTopOf="@+id/lokasiEventS" />    <TextView        android:id="@+id/tvContactHpS"        style="@style/AppTheme.TextMain"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:background="@color/gray_shimmer"        android:layout_marginTop="4dp"        app:layout_constraintEnd_toEndOf="@+id/lokasiEventS"        app:layout_constraintStart_toStartOf="@+id/lokasiEventS"        app:layout_constraintTop_toBottomOf="@+id/lokasiEventS" />    <TextView        android:id="@+id/tvDetailContactHpS"        style="@style/AppTheme.TextMain"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:background="@color/gray_shimmer"        android:layout_marginStart="8dp"        app:layout_constraintEnd_toEndOf="@+id/tvDetailLocS"        app:layout_constraintStart_toEndOf="@+id/tvContactHpS"        app:layout_constraintTop_toTopOf="@+id/tvContactHpS" />    <TextView        android:id="@+id/tvEmailS"        style="@style/AppTheme.TextMain"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginTop="4dp"        android:background="@color/gray_shimmer"        app:layout_constraintEnd_toEndOf="@+id/tvContactHpS"        app:layout_constraintStart_toStartOf="@+id/lokasiEventS"        app:layout_constraintTop_toBottomOf="@+id/tvContactHpS" />    <TextView        android:id="@+id/tvDetailEmailS"        style="@style/AppTheme.TextMain"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginStart="8dp"        android:background="@color/gray_shimmer"        app:layout_constraintEnd_toEndOf="@+id/tvDetailContactHpS"        app:layout_constraintStart_toEndOf="@+id/tvContactHpS"        app:layout_constraintTop_toTopOf="@+id/tvEmailS" />    <TextView        android:id="@+id/tvAuthorS"        style="@style/AppTheme.TextMain"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginTop="4dp"        android:background="@color/gray_shimmer"        app:layout_constraintEnd_toEndOf="@+id/tvEmailS"        app:layout_constraintStart_toStartOf="@+id/lokasiEventS"        app:layout_constraintTop_toBottomOf="@+id/tvEmailS" />    <TextView        android:id="@+id/tvDetailAuthorS"        style="@style/AppTheme.TextMain"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:background="@color/gray_shimmer"        app:layout_constraintEnd_toEndOf="@+id/tvDetailEmailS"        app:layout_constraintStart_toStartOf="@+id/tvDetailEmailS"        app:layout_constraintTop_toTopOf="@+id/tvAuthorS" />    <TextView        android:id="@+id/detailEventS"        android:layout_width="130dp"        android:layout_height="wrap_content"        android:layout_marginTop="24dp"        android:fontFamily="@font/poppins"        android:background="@color/gray_shimmer"        android:textColor="@color/black"        android:textSize="20sp"        android:textStyle="bold"        app:layout_constraintStart_toStartOf="@+id/tvAuthorS"        app:layout_constraintTop_toBottomOf="@+id/tvDetailAuthorS" />    <TextView        android:id="@+id/tvDescEventS"        style="@style/AppTheme.TextMain"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginTop="4dp"        android:layout_marginEnd="16dp"        android:background="@color/gray_shimmer"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="@+id/detailEventS"        app:layout_constraintTop_toBottomOf="@+id/detailEventS" /></androidx.constraintlayout.widget.ConstraintLayout>